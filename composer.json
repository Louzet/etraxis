{
    "name": "etraxis/etraxis",
    "description": "Issue tracking system with customizable workflows.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "support": {
        "forum": "https://gitter.im/etraxis",
        "issues": "https://github.com/etraxis/etraxis/issues",
        "wiki": "https://github.com/etraxis/etraxis/wiki",
        "source": "https://github.com/etraxis/etraxis"
    },
    "require": {
        "php": "^7.1.3",
        "ext-bcmath": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "league/iso3166": "^2.1",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "ramsey/uuid": "^3.7",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/thanks": "^1.0",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1",
        "webinarium/php-dictionary": "^1.1",
        "webinarium/php-properties": "^1.0",
        "webinarium/symfony-lazysec": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/browser-kit": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "eTraxis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eTraxis\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}

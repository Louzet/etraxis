<div id="vue-field" v-cloak>

    <div v-if="applicationId === 'field'" class="grid-70 push-30 mobile-grid-100">
        <div class="grid-row">
            <tabs>
                <tab id="field" title="{{ 'field' | trans }}">
                    {% include 'projects/field/tab_field.html.twig' %}
                </tab>
            </tabs>
        </div>
    </div>

    <modal ref="dlgEditField" header="{{ 'field.edit' | trans }}" @submit="updateField">
        {% include 'projects/field/dlg_field.html.twig' with { new: false } %}
    </modal>

</div>
